equations {set=Euler;}

timeMarching {integrator=backwardEuler; type=CFL; step=5.E-4; CFL=2.; numberOfSteps=1000000; outFreq=50000;}

numericalOptions {order=first; limiter=minmod; sharpeningFactor=0.14;}

initialConditions { 
	region_1(type=box; box_1=[-1,-1,-1]; box_2=[0.5,1,1]; rho=1.; v=[0,0,0]; p=2.;); 
	region_2(type=box; box_1=[0.5,-1,-1]; box_2=[1,1,1]; rho=1.; v=[0,0,0]; p=1.;);
}

boundaryConditions { 
	BC_1 (type=slip; region=box; box_1=[-1,-1,-1]; box_2=[2,2,2]; pick=override;); 
    	BC_2 (type=slip; region=box; box_1=[0,-1,-1]; box_2=[0,2,2]; pick=override;);
	BC_3 (type=slip; region=box; box_1=[1,-1,-1]; box_2=[1,2,2]; pick=override;);
}

fluidProperties {gamma=1.4; viscosity(type=fixed; value=2E-2;);}

solutionFormat {input=tecplot; output=tecplot;}
